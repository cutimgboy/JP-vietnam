# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=  # 如果 Redis 设置了密码，请取消注释并填写
REDIS_DB=0

# WebSocket 行情源配置
QUOTE_WS_TOKEN=35521d6532368e5c6959325e3fa07604-c-app

# 股票行情系统配置
# 目标股票列表（逗号分隔）
TARGET_STOCK_SYMBOLS=NVDA.US,MSFT.US,AAPL.US,AMZN.US,GOOG.US

# Redis 缓存配置
STOCK_QUOTE_CACHE_TTL=60          # 单个股票行情缓存时间（秒）
ALL_QUOTES_CACHE_TTL=2            # 所有股票汇总缓存时间（秒）
SPREAD_CACHE_TTL=300              # 价差设置缓存时间（秒）
PRICE_DETECTION_THRESHOLD=0.0001  # 价格变化检测阈值

# 数据库配置
ENABLE_PRICE_CHANGE_LOG=true      # 是否启用价格变动记录
CLEANUP_OLD_RECORDS_DAYS=30       # 清理旧记录的天数

# 性能配置
MAX_CONCURRENT_DB_WRITES=10       # 最大并发数据库写入数
BATCH_REDIS_UPDATE_SIZE=5         # 批量Redis更新大小

# 监控配置
ENABLE_PERFORMANCE_LOGGING=true   # 是否启用性能日志
SLOW_QUERY_THRESHOLD_MS=100       # 慢查询阈值（毫秒）